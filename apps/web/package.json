    "db:studio:test": "drizzle-kit studio --config=drizzle.test.config.ts",
    "db:test:local": "tsx scripts/db-test-connection.ts local",
    "db:test:preview": "tsx scripts/db-test-connection.ts preview",
    "db:test:prod": "tsx scripts/db-test-connection.ts production",
    "dev": "next dev",
    "lint": "eslint . --max-warnings 0 --config ../../eslint.config.mjs",
    "lint:fix": "eslint . --fix --max-warnings 0 --config ../../eslint.config.mjs",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "clean": "rm -rf .next && rm -rf node_modules",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
