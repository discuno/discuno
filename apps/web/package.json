  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "eslint . && tsc --noEmit",
    "db:generate": "drizzle-kit generate --config=../../drizzle.config.ts",
    "db:generate:local": "drizzle-kit generate --config=../../drizzle.local.config.ts",
    "db:generate:preview": "drizzle-kit generate --config=../../drizzle.preview.config.ts",
    "db:generate:prod": "drizzle-kit generate --config=../../drizzle.production.config.ts",
    "db:push": "tsx scripts/db-push.ts",
    "db:seed": "SKIP_ENV_VALIDATION=1 tsx scripts/db-seed.ts",
    "db:reset:local": "SKIP_ENV_VALIDATION=1 tsx scripts/db-reset.ts local",
    "db:reset:preview": "SKIP_ENV_VALIDATION=1 tsx scripts/db-reset.ts preview",
    "db:reset:test": "SKIP_ENV_VALIDATION=1 tsx scripts/db-reset.ts test",
    "db:studio": "drizzle-kit studio --config=../../drizzle.config.ts",
    "db:studio:local": "drizzle-kit studio --config=../../drizzle.local.config.ts",
    "db:studio:preview": "drizzle-kit studio --config=../../drizzle.preview.config.ts",
    "db:studio:prod": "NODE_TLS_REJECT_UNAUTHORIZED=0 drizzle-kit studio --config=../../drizzle.production.config.ts",
    "db:studio:test": "drizzle-kit studio --config=../../drizzle.test.config.ts",
    "db:test:local": "tsx scripts/db-test-connection.ts local",
    "db:test:preview": "tsx scripts/db-test-connection.ts preview",
    "db:test:prod": "tsx scripts/db-test-connection.ts production",
    "dev": "next dev",
    "lint": "eslint . --max-warnings 0 --config ../../eslint.config.mjs",
    "lint:fix": "eslint . --fix --max-warnings 0 --config ../../eslint.config.mjs",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "clean": "rm -rf .next && rm -rf node_modules",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
